# Snowflake Connection Configuration
# Copy this file to .env and fill in your actual values
# Never commit .env file to git!

SNOWFLAKE_ACCOUNT=your_account_identifier
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_ROLE=ACCOUNTADMIN
SNOWFLAKE_WAREHOUSE=QUICKSTART_WH
SNOWFLAKE_DATABASE=QUICKSTART_prod
SNOWFLAKE_SCHEMA=silver

# Alternative: Use private key authentication (recommended for production)
# SNOWFLAKE_PRIVATE_KEY_FILE=path/to/private_key.pem
# SNOWFLAKE_AUTHENTICATOR=SNOWFLAKE_JWT